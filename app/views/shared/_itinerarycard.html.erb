<div class="container_card">
   <div class="container__info">
    <span><i class="fas fa-thumbs-up"></i>2350</span>
   </div>
  <div class="container__profile">
    <img
     src="https://images.unsplash.com/photo-1438761681033-6461ffad8d80?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80"
     alt="country or city of travel"
    />
    <div class="container__profile__text">
     <h2><%= itinerary.address %></h2>
     <h6><%= itinerary.title %></h6>
     <p>by <b><%= itinerary.user.first_name %></b></p>

      <div class="container_buttons d-flex justify-content-start">
        <div class="pe-1">
          <form action="/itineraries/1/plan">
            <button class="btn button-74" type="submit">edit</button>
          </form>
        </div>
        <div class="pe-1">

        <%= link_to "show", itinerary_path(itinerary), class:"btn button-75" %>
          <%# <form action="/itineraries/1/summary">
            <button class="btn button-75" type="submit">show</button>
          </form> %>
        </div class="">
        <div>
          <form action="/itineraries/1/delete">
            <button class="btn button-75" type="submit">delete</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>



